{
  "name": "eslint-customized-config",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.5.0"
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "browser": true,
      "jquery": true,
      "es6": true
    },
    "rules": {
      "max-len": "off",
      "no-multiple-empty-lines": "off",
      "require-jsdoc": "off",
      "no-var": "error",
      "brace-style": "off",
      "indent": ["error", 2]
    },
    "globals": {
        "chrome": false
    }
  }
}